  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/index.ts",
    "dev:client": "cross-env VITE_PORT=4003 vite",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production vite build --mode production",
    "build:server": "cross-env NODE_ENV=production esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --sourcemap --minify",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch --config jest.config.cjs",
    "test:coverage": "jest --coverage --config jest.config.cjs",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rimraf dist",
    "db:push": "drizzle-kit push",
    "db:migrate": "node scripts/migrate-database.js --migrate",
    "db:fresh": "node scripts/migrate-database.js --fresh",
    "db:status": "node scripts/migrate-database.js --status",
    "postinstall": "npm run build",
    "heroku-postbuild": "npm run build",
    "test:article": "node test-article-api.js"
  },
